{
  "version": "0.2",
  "language": "en",
  "ignorePaths": [
    ".github/workflows/**",
    "**/*.json",
    "**/*.svg",
    "**/*.ai",
    "**/*.csproj",
    "**/CHANGELOG.md",
    "**/node_modules/**",
    "pnpm-lock.yaml"
  ],
  "dictionaries": [
    "en_US",
    "filetypes",
    "misc",
    "node",
    "npm",
    "softwareTerms"
  ],
  "ignoreRegExpList": [
    "/[^\\s@]+@([^\\s@]+\\.)+[^\\s@]{2,4}/",
    "/\\b[A-Fa-f0-9]{6}\\b/",
    "/\\b[A-Za-z0-9]{32,}\\b/",
    "// @ts-.*",
    "/\\STS\\S+/",
    "/`[^`]*`/",
    "/``[\\w\\W]*?``/",
    "/```[\\w\\W]*?```/",
    "/~~~[\\w\\W]*?~~~/",
    "\\(#.+?\\)"
  ],
  "words": [
    "autocode",
    "automerge",
    "BlueBeka",
    "buildblock",
    "noreply",
    "Rebecca",
    "rebeccastevens",
    "resetlockout",
    "streamermode"
  ]
}
